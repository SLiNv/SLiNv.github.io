source 'https://rubygems.org'

group :jekyll_plugins do
    gem 'jekyll-feed'
    gem 'jekyll-archives'
    gem 'jekyll-paginate'
    gem 'jekyll-sitemap'
    gem 'jekyll-gist'
    gem 'jekyll-seo-tag'
    gem 'jekyll-tagging'
    gem 'jekyll-tagging-related_posts'
    gem 'tzinfo-data', platforms: [:mingw, :mswin, :x64_mingw, :jruby]
    gem 'wdm', '~> 0.1.0' if Gem.win_platform?
 end